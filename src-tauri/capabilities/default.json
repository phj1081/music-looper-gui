{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "args": true,
          "name": "binaries/music-looper-sidecar",
          "sidecar": true
        }
      ]
    },
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
